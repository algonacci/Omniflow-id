{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "snap": "react-snap",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check . --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react-snap": "^1.23.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.5.3",
    "vite": "^6.3.5"
  },
  "reactSnap": {
    "include": [
      "/",
      "/en",
      "/id",
      "/zh",
      "/en/modules/hris",
      "/en/modules/ecommerce",
      "/en/modules/xrm",
      "/en/modules/accounting",
      "/en/modules/pos",
      "/en/blog",
      "/en/contact",
      "/id/modules/hris",
      "/id/modules/ecommerce",
      "/id/modules/xrm",
      "/id/modules/accounting",
      "/id/modules/pos",
      "/id/blog",
      "/id/contact",
      "/zh/modules/hris",
      "/zh/modules/ecommerce",
      "/zh/modules/xrm",
      "/zh/modules/accounting",
      "/zh/modules/pos",
      "/zh/blog",
      "/zh/contact"
    ],
    "source": "dist",
    "destination": "dist",
    "headless": "new",
    "minifyHtml": false,
    "skipThirdPartyRequests": true,
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox", "--disable-dev-shm-usage"],
    "puppeteer": {
      "args": ["--no-sandbox", "--disable-setuid-sandbox", "--disable-dev-shm-usage"]
    }
  }
}
